# SportsAnalytics-CV Environment Configuration
# Copy to .env and customize for your environment

# Model settings
MODEL_PATH=models/best.pt
DEVICE=cuda
CONFIDENCE=0.5

# Logging
LOG_LEVEL=INFO

# API settings
API_HOST=0.0.0.0
API_PORT=8000

# Security — set API_KEY to enable authentication (leave empty to disable)
API_KEY=

# CORS — comma-separated list of allowed origins
CORS_ORIGINS=http://localhost:3000,http://localhost:8501

# Rate limiting — max requests per minute per IP (0 = disabled)
RATE_LIMIT_RPM=60
